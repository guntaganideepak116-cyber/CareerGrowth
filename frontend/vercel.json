{
    "crons": [
        {
            "path": "/api/cron/daily-engagement",
            "schedule": "0 9 * * *"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "unsafe-none"
                },
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "unsafe-none"
                },
                {
                    "key": "Cross-Origin-Resource-Policy",
                    "value": "cross-origin"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}